A = [11;22;33]

horzcat(A,B)


function C = reshape_groups(groups, data)
[row, col] = size(data);

group = zeros(col, 1);
C = [];

for i = 1:row
    new = reshape(data(i,:),[col,1]);

    for j = 1:row
        group(j) = groups(i);
    end

    C = [C; horzcat(group, new)];
end
